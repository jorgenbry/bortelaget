{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/weather",
            "dest": "/server.js"
        },
        {
            "src": "/icons/(.*)",
            "dest": "/server.js"
        }
    ]
} 